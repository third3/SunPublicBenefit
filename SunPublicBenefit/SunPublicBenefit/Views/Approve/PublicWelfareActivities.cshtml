    <script type="text/javascript">
		window.onload=function () {
			var onLine=document.getElementById('onlineto');//更多的推荐活动
			var offLine=document.getElementById('endoffline');//更多的线上活动
			var oA=document.getElementById('A');
			var oB=document.getElementById('B');
			var oC=document.getElementById('C');
			function tJ(){
				oA.style.overflow='none';
				oA.style.height='auto';
				onLine.style.display='none';
				offLine.innerHTML='';
				onLine.innerHTML='';
				oB.style.display='none';
				oA.style.display='block';
				oC.style.display='none';
			}
			function onlineto(){
				oB.style.overflow='none';
				oB.style.height='auto';
				offLine.innerHTML='';
				onLine.innerHTML='';
				onLine.style.display='none';
				oB.style.display='block';
				oA.style.display='none';
				oC.style.display='none';
			}

			onLine.onclick=function(){
				tJ();
			}
			offLine.onclick=function(){
				onlineto();
			}

			var aA=document.getElementById('menu').getElementsByTagName('a');
			    var i=0;

			    for(i=0;i<aA.length;i++)
			    {
			        aA[i].iTime=null;
			        aA[i].iSpeed=6;
			        aA[i].onmouseover=function ()
			        {
			            goTime(this,30,1);
			        }
			        aA[i].onmouseout=function ()
			        {
			            goTime(this,6,-1);
			        }
			    }
			    aA[0].onclick=function(){
			    	oB.style.display='block';
					oA.style.display='block';
					oC.style.display='block';
					oA.style.overflow='hidden';
					oB.style.overflow='hidden';
					oC.style.overflow='hidden';
					oA.style.height=425+'px';
					oB.style.height=425+'px';
					oC.style.height=550+'px';
			    }
			    aA[1].onclick=function(){
			        	tJ();
			    }
			    aA[2].onclick=function(){
			    	onlineto();
			    }
			    aA[3].onclick=function(){
			    	offLine.innerHTML='';
					onLine.innerHTML='';
					oC.style.overflow='none';
					oC.style.height='auto';
					oB.style.display='none';
					oA.style.display='none';
					oC.style.display='block';
			    }

			function goTime(obj,iTarget,toggle){
			    if(obj.iTime)
			    {
			        clearInterval(obj.iTime);
			    }
			    obj.iTime=setInterval(function()
			    {
			        if(obj.iSpeed===iTarget)
			        {
			            clearInterval(obj.iTime);
			            obj.iTime=null;
			        }
			        else
			        {
			            obj.iSpeed+=2*toggle;
			            obj.style.paddingLeft=obj.iSpeed+'px';
			            obj.style.paddingRight=obj.iSpeed+'px';

			        }

			    },30);
			}
		}

    </script>
